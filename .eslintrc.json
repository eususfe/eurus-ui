{
  "plugins": [
    "jsx-a11y"
  ],
  "extends": [
    "@estjs/eslint-config",
    "plugin:jsx-a11y/recommended"
  ],
  "rules": {
    "no-unused-vars": 0,
    "import/no-named-as-default": 0
  },
  "settings": {
    "jsx-a11y": {
      "components": {
        "ButtonElement": "button"
      }
    }
  }
}
