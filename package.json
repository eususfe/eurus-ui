{
  "name": "eurus-ui",
  "version": "0.0.0",
  "description": "",
  "type": "module",
  "private": true,
  "packageManager": "pnpm@7.1.7",
  "engines": {
    "node": ">= 14"
  },
  "main": "dist/lib/index.js",
  "module": "dist/es/index.js",
  "scripts": {
    "dev": "pnpm run clean && pnpm run gen-version && esno ./scripts/rollup-watch.ts",
    "build:compts": "pnpm run clean && esno ./scripts/gen-compts.ts",
    "build:esm": "pnpm run gen-version && cross-env MODULE_ENV=esm esno ./scripts/build-module.ts",
    "build:cjs": "pnpm run gen-version && cross-env MODULE_ENV=cjs esno ./scripts/build-module.ts",
    "build:full": "pnpm run clean && pnpm run build:esm && pnpm run build:cjs && esno scripts/full-build.ts",
    "build:all": "esno scripts/full-build.ts",
    "gen-version": "esno ./scripts/gen-version.ts",
    "clean": "rimraf dist lib es",
    "docs:dev": "vitepress dev docs",
    "docs:build": "pnpm -C docs build",
    "docs:serve": "pnpm -C docs serve",
    "commit": "git-cz",
    "release": "standard-version",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "log": "conventional-changelog",
    "lint-change": "lcf --fix \"{src,build,scripts,demo}/**/*.{ts,tsx,js,vue}\"",
    "lint": "eslint --fix \"{src,scripts}/**/*.{ts,tsx,js,vue}\"",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jiangxd2016/eurus-ui.git"
  },
  "keywords": [
    "vue ui"
  ],
  "author": "jiangxd",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jiangxd2016/eurus-ui/issues"
  },
  "homepage": "https://github.com/jiangxd2016/eurus-ui#readme",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "prepare-commit-msg": "exec < /dev/tty && npx git-cz --hook"
    }
  },
  "lint-staged": {
    "**/*.{js,ts,vue}": [
      "eslint --fix"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "pnpm": {
    "overrides": {
      "vite": "^2.9.9",
      "eurus-ui": "workspace:*"
    }
  },
  "peerDependencies": {
    "vue": "^3.2.26"
  },
  "dependencies": {
    "async-validator": "^4.1.1",
    "dayjs": "^1.11.2"
  },
  "devDependencies": {
    "@estjs/eslint-config": "^0.3.1",
    "@fullhuman/postcss-purgecss": "^4.1.3",
    "@rollup/plugin-commonjs": "^22.0.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-replace": "^4.0.0",
    "@types/jest": "^28.1.1",
    "@types/node": "^17.0.40",
    "@types/stylus": "^0.48.38",
    "@vitejs/plugin-vue": "^2.3.3",
    "@vitejs/plugin-vue-jsx": "^1.3.10",
    "@vue/compiler-sfc": "^3.2.36",
    "@vue/test-utils": "^2.0.0",
    "chokidar": "^3.5.3",
    "vite": "^2.9.9",
    "cross-env": "^7.0.3",
    "cssnano": "^5.1.11",
    "cz-conventional-changelog": "^3.3.0",
    "esbuild": "^0.14.42",
    "eslint": "^8.17.0",
    "esno": "^0.16.3",
    "klaw-sync": "^6.0.0",
    "ora": "^6.1.0",
    "pnpm": "7.1.7",
    "postcss": "^8.4.14",
    "postcss-import": "^14.1.0",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.75.5",
    "rollup-plugin-esbuild": "^4.9.1",
    "rollup-plugin-filesize": "^9.1.2",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-purge-icons": "^0.8.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-ts": "^3.0.1",
    "rollup-plugin-vue": "^6.0.0",
    "stylus": "^0.58.1",
    "ts-morph": "^15.1.0",
    "typescript": "^4.7.3",
    "vitest": "^0.13.1"
  }
}
