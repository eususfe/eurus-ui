{
  "name": "eurus-ui",
  "version": "0.0.0",
  "description": "",
  "type": "module",
  "private": true,
  "packageManager": "pnpm@7.1.7",
  "engines": {
    "node": ">= 14"
  },
  "main": "dist/lib/index.js",
  "module": "dist/es/index.js",
  "scripts": {
    "dev": "esno scripts/build.ts",
    "build:compts": "cross-env NODE_ENV=compts esno scripts/build.ts",
    "build:all": "cross-env NODE_ENV=all esno scripts/build.ts",
    "build-types": "pnpm run build-types-cjs && pnpm run build-types-esm",
    "build-types-cjs": "vue-tsc -p ./src/tsconfig.cjs.json --declaration --emitDeclarationOnly",
    "build-types-esm": "vue-tsc -p ./src/tsconfig.esm.json --declaration --emitDeclarationOnly",
    "gen-version": "esno ./scripts/gen-version.ts",
    "docs:dev": "pnpm -C docs dev",
    "docs:build": "pnpm -C docs build",
    "docs:serve": "pnpm -C docs serve",
    "changelog": "esno scripts/changelog.ts",
    "clean": "rimraf dist",
    "lint-change": "lcf --fix \"{src,build,scripts,demo}/**/*.{ts,tsx,js,vue}\"",
    "lint": "eslint --fix \"{src,scripts}/**/*.{ts,tsx,js,vue}\"",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jiangxd2016/eurus-ui.git"
  },
  "keywords": [
    "ui",
    "vue",
    "eurus-ui",
    "ui framework",
    "component library"
  ],
  "author": "jiangxd",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jiangxd2016/eurus-ui/issues"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-git"
    }
  },
  "homepage": "https://github.com/jiangxd2016/eurus-ui#readme",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "prepare-commit-msg": "exec < /dev/tty && npx git-cz --hook"
    }
  },
  "lint-staged": {
    "**/*.{js,ts,vue}": [
      "eslint --fix"
    ]
  },
  "peerDependencies": {
    "async-validator": "^4.1.1",
    "dayjs": "^1.11.3",
    "vue": "^3.2.26"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.3.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@estjs/eslint-config": "^0.3.1",
    "@types/node": "^17.0.42",
    "@vitejs/plugin-vue": "^2.3.3",
    "@vitejs/plugin-vue-jsx": "^1.3.10",
    "@vue/test-utils": "^2.0.0",
    "c8": "^7.11.3",
    "chokidar": "^3.5.3",
    "cross-env": "^7.0.3",
    "cypress": "^9.7.0",
    "eslint": "^8.17.0",
    "esno": "^0.16.3",
    "pnpm": "7.1.7",
    "rimraf": "^3.0.2",
    "sass": "^1.52.3",
    "standard-changelog": "^2.0.27",
    "typescript": "^4.7.3",
    "vite": "^3.0.0-alpha.10",
    "vitest": "^0.13.1",
    "vue": "^3.2.37",
    "vue-tsc": "^0.35.2",
    "vue-types": "^4.1.1"
  }
}
